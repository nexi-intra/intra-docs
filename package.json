{
  "name": "@koksmat/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules ",
    "docusaurus": "docusaurus",
    "gendoc": "docusaurus gen-api-docs all",
    "development": "export debug= && docusaurus start -p 3001",
    "build": "export debug=* && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve -p 4444",
    "inst": "pnpm install",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^1.1.0",
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@docusaurus/theme-mermaid": "^3.5.2",
    "@mdx-js/react": "^3.0.1",
    "add": "^2.0.6",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "docusaurus-plugin-openapi-docs": "^3.0.2",
    "docusaurus-theme-openapi-docs": "^3.0.2",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@tsconfig/docusaurus": "^2.0.3",
    "docusaurus-plugin-typedoc": "^1.0.5",
    "typedoc": "^0.26.6",
    "typedoc-plugin-markdown": "^4.2.5",
    "typedoc-plugin-zod": "^1.2.1",
    "typescript": "^5.5.4"
